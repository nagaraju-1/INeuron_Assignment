   "source": [
    "## Pattern printing"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "\n",
      "*\n",
      "**\n",
      "***\n",
      "****\n",
      "*****\n",
      "****\n",
      "***\n",
      "**\n",
      "*\n"
     ]
    }
   ],
   "source": [
    "r=5\n",
    "for i in range(r+1):\n",
    "    print(i*'*')\n",
    "    if i==r:\n",
    "        for j in range(r-1,0,-1):\n",
    "            print(j*'*')"
   ]
  },
